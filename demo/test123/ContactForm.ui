    <annotations source="urn:ooo">
      <contents key="olivier" value="moises"/>
    </annotations>
    <styleRules xsi:type="swtstyles:GridLayoutRule" propertyName="layout" numColumns="2"/>
    <children xsi:type="wcw:Label">

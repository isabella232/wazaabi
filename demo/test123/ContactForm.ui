      <handlers xsi:type="edphdlrs:Binding">
        <parameters xsi:type="edphdlrs:StringParameter" name="source" value="$input/@gender"/>
        <parameters xsi:type="edphdlrs:StringParameter" name="target" value="@selection"/>

    <children xsi:type="wcw:TextComponent">
      <handlers xsi:type="edphdlrs:Binding">
        <parameters xsi:type="edphdlrs:StringParameter" name="source" value="$input/@lastName"/>
        <parameters xsi:type="edphdlrs:StringParameter" name="target" value="@text"/>
        <events xsi:type="edpevts:PropertyChangedEvent" path="$input/@lastName"/>
        <events id="core:ui:refresh"/>
      </handlers>
      <handlers xsi:type="edphdlrs:Binding">
        <parameters xsi:type="edphdlrs:StringParameter" name="source" value="@text"/>
        <parameters xsi:type="edphdlrs:StringParameter" name="target" value="$input/@lastName"/>
        <events id="core:ui:focus:out"/>
      </handlers>
      <styleRules xsi:type="swtstyles:GridDataRule" propertyName="layout-data" grabExcessHorizontalSpace="true" horizontalAlignement="FILL"/>
      <styleRules xsi:type="wcs:BooleanRule" propertyName="border" value="true"/>
    </children>
    <children xsi:type="wcw:Label">
      <styleRules xsi:type="wcs:StringRule" propertyName="text" value="company"/>
      <styleRules xsi:type="swtstyles:GridDataRule" propertyName="layout-data"/>
    </children>
    <children xsi:type="wcw:TextComponent">
      <handlers xsi:type="edphdlrs:Binding">
        <parameters xsi:type="edphdlrs:StringParameter" name="source" value="$input/@company"/>
        <parameters xsi:type="edphdlrs:StringParameter" name="target" value="@text"/>
        <events xsi:type="edpevts:PropertyChangedEvent" path="$input/@company"/>
        <events id="core:ui:refresh"/>
      </handlers>
      <handlers xsi:type="edphdlrs:Binding">
        <parameters xsi:type="edphdlrs:StringParameter" name="source" value="@text"/>
        <parameters xsi:type="edphdlrs:StringParameter" name="target" value="$input/@company"/>
        <events id="core:ui:focus:out"/>
      </handlers>
